<action_code>public ActionForward perform_select_action(ActionMapping mapping, ActionForm form, HttpServletRequest request, HttpServletResponse response) {
  HttpSession session = request.getSession(true);
  ActionForward actionForward = null;
  try {
    NormasVendaActionForm f = (NormasVendaActionForm) form;
    if(f.getColecao().equals("0")){
        throw new Exception("Selecione a coleção!");
    }
    actionForward = new ActionForward("/com/NormasVenda_cForm.do?select_action=&lidi_marca="+f.getColecao(),true);
  } catch (Exception e) {
    ActionErrors errors = new ActionErrors();
    errors.add("ActionErrors.GLOBAL_ERROR_bl_form", new ActionMessage("error.action.exception",com.egen.util.system.Error.getMessage(e)));
    request.setAttribute(org.apache.struts.Globals.ERROR_KEY, errors);
    session.setAttribute("exception", com.egen.util.system.Error.getDescription(e));
    actionForward = mapping.findForward("same");
  }
  return actionForward;
}
