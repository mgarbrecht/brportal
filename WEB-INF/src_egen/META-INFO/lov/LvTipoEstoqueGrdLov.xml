     if(session.getAttribute("codigos_estoque")!=null){
        tiposEstoqueSelecionados = (String)session.getAttribute("codigos_estoque");
        tt = tiposEstoqueSelecionados.split(",");
     }
  }
  if (f != null) {
    Jw_tipo_estoque table = new Jw_tipo_estoque();
