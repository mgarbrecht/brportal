<fieldRelationCode>public String getUsua_nome() {
com.egen.util.jdbc.JdbcUtil j = null;
try {
  if (usua_usuario !=null && usua_usuario.length()>0) {
    j = new com.egen.util.jdbc.JdbcUtil();
    Usuarios db_object = new Usuarios();
    String[][] select = {
      {"usua_nome",null},
      {"usua_id",null},
      {"situacao",null}
      };
    Object[][] where = {
      {"usua_usuario","like",usua_usuario},
      };
    String[] group = null;
    String[] having = null;
    String[] order = null;
    java.util.Vector results = j.select(db_object, select, where, group, having, order);
    if (results!=null && results.size()>0) {
      db_object = (Usuarios)results.elementAt(0);
      usua_nome = (db_object.getUsua_nome()!=null)?db_object.getUsua_nome()+"":"";
      usua_id = (db_object.getUsua_id()!=null)?db_object.getUsua_id() + "":"";
      situacao = (db_object.getSituacao()!=null)?db_object.getSituacao()+"":"";
      if(situacao.equalsIgnoreCase("A")){
         situacao = "Ativo";
      } else {
         situacao = "Bloqueado";
      }
    } else {
      usua_nome = "";
      usua_id = "";
      situacao = "";
    }
  }
} catch (Exception e) {
      usua_nome = "";
      usua_id = "";
      situacao = "";
} finally {
  if (j!=null) { j.close(); }
}
return usua_nome;
}

