      int codigo_regional = 0;
      int contador =0;

      t_jw_maiores_clientes_vencidos = (portalbr.dbobj.view.Jw_maiores_clientes_vencidos)res_Jw_maiores_clientes_vencidos.elementAt(i_bl_report_Jw_maiores_clientes_vencidos);
