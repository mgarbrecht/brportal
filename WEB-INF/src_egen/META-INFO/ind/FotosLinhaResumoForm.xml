<triggerCode>private void tg_select_action(ActionMapping mapping, ActionForm form, HttpServletRequest request, HttpServletResponse response) throws Exception {
  HttpSession session = request.getSession(true);
  try {
    FotosLinhaResumoActionForm f = (FotosLinhaResumoActionForm) form;
    acesso.Usuario usuario = (acesso.Usuario)session.getAttribute("User");
    if(usuario.mostraQuery()){
       throw new Exception("Query não disponível! Javaview: Jw_ficha_tec_imagem");
    }
    session.setAttribute(mapping.getName(), form);
  } catch (Exception e) {
    throw e;
  } finally {
  }
}

