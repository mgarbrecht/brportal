<$key>query</$key>
<$value>SELECT p.ped_nmro
             , i.lin_cdgo
			 , i.ref_cdgo
			 , i.cab_cdgo
			 , ca.cab_desc
			 , i.cor_cdgo
			 , co.cor_desc
			 , 0 quantidade
			 , p.cli_cdgo
			 , p.esc_seqn
			 , i.ite_seqn
             , i.ite_prco			 
			 , p.ped_boni desconto_comercial
			 , ped_boni2 desconto_promocional
			 , pck_calculos.subtrai_percentual(ite_prco, (ped_boni+ped_boni2)) ite_prco_descontos			 
   FROM ped p
      , ite i
      , cabedal ca
      , cor co
  WHERE p.ped_nmro = i.ped_nmro
    AND co.cor_cdgo = i.cor_cdgo
    AND ca.cab_cdgo = i.cab_cdgo
    AND p.ped_stat = 'F'
  ORDER BY ite_seqn
 </$value>
</properties_item>
