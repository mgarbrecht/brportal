                ,cr.codigo_cliente
                ,c.cli_rzao
                ,c.cli_nome
                ,s.esc_seqn
                ,cr.codigo_representante
                ,r.rep_rzao
                ,r.rep_nome
                ,'' cliente_desde
				,'' ultima_compra
				,'' email_cliente
				,'' fone_cliente
				,'' gre_grupo
				,'' gre_descricao
				,'' marcas_representante
   FROM cliente_representante cr
       ,regionais_vendas rv
       ,rep r
       ,cli c
       ,esc s
  WHERE cr.codigo_representante = r.rep_cdgo
    AND s.cli_cdgo = c.cli_cdgo
    AND cr.codigo_cliente = c.cli_cdgo
    AND rv.codigo_regional = r.codigo_regional
    AND r.rep_situ = 'A'
    AND cr.situacao = 'A'
    AND cr.codigo_sequencial = s.esc_seqn</$value>
</properties_item>
<properties_item>
<$key>metadata</$key>
<$value></$value>
</properties_item>
</properties>
