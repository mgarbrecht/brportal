<$key>query</$key>
<$value>SELECT DISTINCT ecl.cli_cdgo
               ,ecl.ecl_cdad || ' - ' || ecl.est_unifed cidade
               ,NVL(esc.esc_email, '-') esc_email
               ,nvl(rowtocol('SELECT ffs_ddd2 || '' - '' || ffs_nmro2 fone FROM fax_fon2 WHERE cli_cdgo = ' || ecl.cli_cdgo || '  AND esc_seqn = ' || ecl.esc_seqn || '  AND ned_cdgo = ''' || ecl.ned_cdgo || '''  AND cli_cdgo > 0'),'-') fones
  FROM ecl
      ,esc
 WHERE ecl.ned_cdgo IN ('ENT', 'UNI')
   AND esc.cli_cdgo = ecl.cli_cdgo
   AND esc.esc_seqn = ecl.esc_seqn
   AND esc.esc_situacao = 'A'
 ORDER BY 2
</$value>
