<$key>query</$key>
<$value>SELECT distinct mp.codigo_marca
      ,mp.descricao_marca
      ,lin_cdgo
      ,ref_cdgo
      ,fct_mask_modelo(lin_cdgo, ref_cdgo) modelo
  FROM ref_cor rc
      ,marcas_produtos mp
 WHERE mp.codigo_marca = fct_marca_produto(rc.lin_cdgo, rc.ref_cdgo)
   AND rc.rfc_status = 'A'
ORDER BY lin_cdgo, ref_cdgo
  </$value>
