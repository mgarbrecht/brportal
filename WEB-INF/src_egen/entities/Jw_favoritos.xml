<$key>query</$key>
<$value>SELECT m.menu_nome
      ,adi.acdi_usua_id
      ,i.itme_id
      ,i.itme_texto
      ,REPLACE(REPLACE(i.itme_comando, '<doc_interface>', di.doin_nome), '<app_context>', '..') link
  FROM acessos_documentos_interfaces adi
      ,documentos_interfaces di
      ,wbrio.itens_menus i
      ,wbrio.menus m
 WHERE adi.acdi_doin_id = di.doin_id
   AND i.itme_doin_id = di.doin_id
   AND adi.acdi_usua_id > 0
   AND m.menu_id = i.itme_menu_id
   AND adi.acdi_favorito = 'S'
   AND NVL(m.menu_codigo_sistema, 'BRNET') = 'BRNET'
   AND NVL(i.itme_menu_codigo_sistema, 'BRNET') = 'BRNET'
</$value>
