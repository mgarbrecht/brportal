<$key>query</$key>
<$value>SELECT DISTINCT a.cli_cdgo,b.esc_seqn,c.ned_cdgo,c.ecl_nome||' , '||c.ecl_nmro endereco,  substr(c.ecl_cep,1,5)||'-'||substr(c.ecl_cep, 6,3)cep,  c.ecl_bair||' - '||c.ecl_cdad||'/'||c.est_unifed bairro_cidade  FROM BRIO.CLI A, BRIO.ESC B, BRIO.ECL C, CLIENTE_REPRESENTANTE CR  WHERE B.ESC_SEQN=C.ESC_SEQN   AND A.CLI_CDGO=B.CLI_CDGO   AND B.CLI_CDGO=C.CLI_CDGO   AND CR.CODIGO_CLIENTE=A.CLI_CDGO   AND CR.CODIGO_SEQUENCIAL=B.ESC_SEQN  AND a.cli_situ='A'  AND b.esc_situacao='A'  AND cr.situacao='A'  AND A.CLI_CDGO=3130   AND b.esc_seqn=2</$value>
