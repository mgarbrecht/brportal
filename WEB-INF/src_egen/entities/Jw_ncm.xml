<$key>query</$key>
<$value>SELECT ref.codigo_marca
      ,pck_marcas_produtos.get_nome(ref.codigo_marca) marca_nome
      ,d.lin_cdgo
      ,d.ref_cdgo
      ,d.cab_cdgo
      ,d.descricao
      ,i.ipi_posi || i.ipi_subp || LPAD(i.ipi_item, 2, 0) ncm
      ,ref.ref_status
      ,rc.rfb_status
  FROM dom_item_estoque d
      ,ipi i
      ,brio.ref
      ,ref_cab rc
 WHERE i.ipi_codred = d.ipi_codred
   AND ref.lin_cdgo = d.lin_cdgo
   AND ref.ref_cdgo = d.ref_cdgo
   AND rc.lin_cdgo = d.lin_cdgo
   AND rc.ref_cdgo = d.ref_cdgo
   AND rc.cab_cdgo = d.cab_cdgo
</$value>
