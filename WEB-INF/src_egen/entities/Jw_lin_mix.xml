<$key>query</$key>
<$value>SELECT lin_cdgo
      , lin_nome
      , mix
      , case when mix = 'M' then 'MIX' else 'FORA MIX' end descricao_mix
  FROM (SELECT l.lin_cdgo
             , l.lin_nome
             , nvl((SELECT DISTINCT 'M' status
                      FROM ref_cor_mix rc
                     WHERE data_posicao = trunc(sysdate)
                       AND rc.lin_cdgo = l.lin_cdgo
                       AND rc.rfc_status = 'A')
               ,'F') mix
          FROM lin l)
    ORDER BY lin_cdgo
 
 </$value>
