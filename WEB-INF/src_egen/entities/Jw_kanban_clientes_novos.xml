<$key>query</$key>
<$value>SELECT cnpj
       ,codigo_regional
       ,sigla_regional 
       ,CASE
         WHEN (trunc(SYSDATE - dt_inclusao) < 3) THEN
          'VERDE'
         WHEN (trunc(SYSDATE - dt_inclusao) BETWEEN 3 AND 5) THEN
          'AMARELO'
         WHEN (trunc(SYSDATE - dt_inclusao) > 5) THEN
          'VERMELHO'
       END situacao
   FROM webstore.wt_cliente_kanban_vw@sadig
  WHERE atendido = 'N'
 </$value>
