<$key>query</$key>
<$value>SELECT a.prazo_final data_entrega
       ,a.seq_acao_mkt acao
       ,e.seq_evento seq
       ,a.cdgo_tmidia codigo_material
       ,tm.desc_tmidia desc_material
       ,a.situacao_acao_mkt situacao
       ,a.descr_imagem imagem
       ,a.especificacao_tmidia formato
       ,a.obs_acao_mkt observacao
       ,e.email
       ,e.obs_evento
       ,e.data_transacao
       ,e.usuario_transacao
       ,e.cracha_usuario
       ,a.codigo_marca
       ,pck_marcas_produtos.get_nome(a.codigo_marca) descricao_marca     
       ,a.cli_cdgo
       ,pck_cliente.get_nome(a.cli_cdgo) cli_rzao
       ,a.rep_cdgo
       ,pck_representante.get_razao_social(a.rep_cdgo) rep_rzao
   FROM acao_marketing a
      , acao_marketing_eventos e
      , tipo_midia tm
  WHERE a.prazo_final IS NOT NULL
    AND e.seq_acao_mkt = a.seq_acao_mkt
    AND tm.cdgo_tmidia = a.cdgo_tmidia
  ORDER BY a.prazo_final 
         , a.seq_acao_mkt
         , e.seq_evento DESC</$value>
