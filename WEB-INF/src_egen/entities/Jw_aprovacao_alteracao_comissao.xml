<$key>query</$key>
<$value>SELECT p.id_solicitacao
       ,initcap(p.usuario_solicitacao) usuario_solicitacao
       ,p.data_solicitacao
       ,nf.rep_cdgo
       ,initcap(r.rep_rzao) rep_rzao
       ,nf.cli_cdgo
       ,nf.esc_seqn
       ,initcap(cli.cli_rzao) cli_rzao
       ,nf.emp_empresa
       ,nf.fil_filial
       ,nf.nfs_nmro
       ,nf.nfs_serie
       ,NVL(nfs_vltot, 0) valor_total
       ,p.valor_calculado
       ,initcap(p.motivo) motivo
	   ,(SELECT SUM(valor_lancamento) FROM brio.solicita_prorrog_lancamentos WHERE id_solicitacao = p.id_solicitacao ) valor_aprovado_parcial
   FROM nf_saida             nf
       ,cli                  cli
       ,rep                  r
       ,brio.solicita_prorrogacao p
  WHERE cli.cli_cdgo = nf.cli_cdgo
    AND r.rep_cdgo = nf.rep_cdgo
    AND p.situacao = 'P'
    AND p.emp_empresa = nf.emp_empresa
    AND p.fil_filial = nf.fil_filial
    AND p.nfs_nmro = nf.nfs_nmro
    AND p.nfs_serie = nf.nfs_serie</$value>
</properties_item>
<properties_item>
<$key>metadata</$key>
<$value></$value>
</properties_item>
</properties>
