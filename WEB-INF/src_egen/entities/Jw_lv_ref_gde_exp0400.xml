<$key>query</$key>
<$value>SELECT rgde.gde_cdgo
       ,grde.gde_numi
       ,grde.gde_numf
       ,rgde.lin_cdgo
       ,rgde.ref_cdgo
       ,p.codigo_corrug
  FROM ref_gde rgde
      ,gre grde
      ,peso_corrugados p
 WHERE grde.gde_cdgo = rgde.gde_cdgo
   AND (1 = 1 OR rgde.rfg_status = 'A')
   AND p.lin_cdgo = rgde.lin_cdgo
   and p.ref_cdgo = rgde.ref_cdgo
   and MOD(grde.gde_tota, p.qt_par_corrug) = 0
   ORDER BY 1 </$value>
