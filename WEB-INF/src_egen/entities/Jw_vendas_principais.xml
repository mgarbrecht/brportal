<$key>query</$key>
<$value>select marca, periodo, sum(pares)pares, sum(valor)valor from( select decode(p.codigo_marca,1,'VIZZANO','BEIRA RIO')marca,   to_char(trunc(data_dig,'MM'),'MM/YYYY')periodo,   sum(pares)pares,   sum(valor)valor from resumo_ped p,  grupo_economico g where data_dig>=to_date('10/10/2005','dd/mm/yyyy')  and data_dig < to_date('01/03/2006','dd/mm/yyyy')+ 1  and p.cli_cdgo=g.cli_cdgo  and g.gre_grupo=10 group by p.codigo_marca, trunc(data_dig,'MM') UNION ALL select decode(p.codigo_marca,1,'VIZZANO','BEIRA RIO')marca,   to_char(trunc(data_dig,'MM'),'MM/YYYY')periodo,   sum(pares)pares,   sum(valor)valor from resumo_pedidos p,  grupo_economico g where data_dig>=to_date('10/10/2005','dd/mm/yyyy')  and data_dig < to_date('01/03/2006','dd/mm/yyyy')+ 1  and p.cli_cdgo=g.cli_cdgo  and g.gre_grupo=10 group by p.codigo_marca, trunc(data_dig,'MM') ) group by marca, periodo</$value>
