<$key>query</$key>
<$value>select pck_linha.get_nome(i.lin_cdgo) AS linha,
       pck_referencia.get_nome(i.lin_cdgo, i.ref_cdgo) AS referencia,
       pck_cabedal.get_nome(i.cab_cdgo) AS cabedal,
       pck_cor.get_nome(i.cor_cdgo) AS cor,
       i.ite_seqn AS sequencial,
       i.lin_cdgo AS codigo_linha,
       i.ref_cdgo AS codigo_referencia,
       i.cab_cdgo AS codigo_cabedal,
       i.cor_cdgo AS codigo_cor,
       p.ped_nmro,
       'Ref: ' || i.lin_cdgo || '.' || i.ref_cdgo || '.' || i.cab_cdgo || ' - ' || i.cor_cdgo ||' '|| i.ite_descricao || ', COR ' || pck_cor.get_nome(i.cor_cdgo) AS descricao_item
  from ite i, ped p
 where i.ped_nmro = p.ped_nmro
</$value>
