<$key>query</$key>
<$value>SELECT cgt.rv_low_value codigo_tipo
        ,cgt.rv_meaning   descricao_tipo
        ,cgs.rv_low_value codigo_subtipo
        ,cgs.rv_meaning descricao_subtipo
    FROM cg_ref_codes cgt
       , cg_ref_codes cgs
   WHERE cgt.rv_domain = 'TIPO_SOLICITACAO_COMB_ESPECIAL'
     and cgs.rv_domain = 'SUBTIPO_SOLICITACAO_COMB_ESPEC'
     AND cgt.rv_low_value IN ('1', '2', '3', '5', '13')
     and cgs.rv_high_value = cgt.rv_low_value
   ORDER BY to_number(cgt.rv_low_value)
          , to_number(cgs.rv_low_value)
 </$value>
