<$key>query</$key>
<$value>SELECT /*+ all_rows */
         DISTINCT
         rv.codigo_regional,
         rv.nome_regional, 
         r.rep_cdgo, 
         r.rep_rzao, 
         c.cli_cdgo cli_cdgo, 
         c.cli_rzao cli_rzao, 
         decode(c.cli_valor_credito_analista,null,0,c.cli_valor_credito_analista,c.cli_valor_credito_analista)  limite, 
         nvl(e.esc_situacao,'A') situacao, 
         a.situacao situacao_cr 
    FROM cliente_representante a, 
         cli c, 
         esc e, 
         rep r, 
         regionais_vendas rv 
   WHERE a.codigo_cliente       =  c.cli_cdgo 
     AND c.cli_cdgo             =  e.cli_cdgo 
     AND a.codigo_cliente       =  e.cli_cdgo 
     AND a.codigo_sequencial    =  e.esc_seqn 
     AND a.codigo_cliente       not in (5239,20698,5248,3284,10758,5247,5236,25126,5256,5271) 
     AND r.rep_cdgo             =  a.codigo_representante 
     AND r.codigo_regional      =  rv.codigo_regional 
   ORDER BY 
         codigo_regional,
         rep_cdgo, 
         situacao, 
         cli_rzao, 
         cli_cdgo </$value>
</properties_item>
