<$key>query</$key>
<$value>SELECT p.ped_nmro
        ,p.cli_cdgo
        ,p.esc_seqn
        ,c.cli_rzao
        ,p.ped_daem
        ,i.ite_seqn
        ,i.lin_cdgo
        ,i.ref_cdgo
        ,i.cab_cdgo
        ,i.cor_cdgo
        ,i.ite_preco_venda
        ,p.ped_ped_cliente
        ,r.rep_cdgo
        ,r.codigo_regional		
    FROM ped p
        ,ite i
        ,cli c
		,rep r
   WHERE p.ped_nmro = i.ped_nmro
     AND p.cli_cdgo = c.cli_cdgo
	 AND p.rep_cdgo = r.rep_cdgo
     AND p.ped_stat NOT IN ('C', 'F', 'L')
   ORDER BY ped_daem DESC
           ,p.ped_nmro
           ,i.ite_seqn</$value>
</properties_item>
