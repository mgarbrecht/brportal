<$key>query</$key>
<$value>SELECT to_char(NVL((SELECT sem.data_mapa
              FROM semanas sem
             WHERE data = (SELECT sem.data_mapa
                             FROM semanas sem
                            WHERE data = TRUNC(CASE WHEN (EXTRACT(DAY FROM SYSDATE) = 1 AND EXTRACT(hour FROM CAST(SYSDATE AS TIMESTAMP))<8) THEN trunc(SYSDATE)-1 ELSE trunc(SYSDATE) END)) - 1), TRUNC(CASE WHEN (EXTRACT(DAY FROM SYSDATE) = 1 AND EXTRACT(hour FROM CAST(SYSDATE AS TIMESTAMP))<8) THEN trunc(SYSDATE)-1 ELSE trunc(SYSDATE) END) - 1),'dd/mm/yyyy') dia_anterior
       ,to_char(NVL((SELECT sem.data_mapa
              FROM semanas sem
             WHERE data = TRUNC(CASE WHEN (EXTRACT(DAY FROM SYSDATE) = 1 AND EXTRACT(hour FROM CAST(SYSDATE AS TIMESTAMP))<8) THEN trunc(SYSDATE)-1 ELSE trunc(SYSDATE) END)), TRUNC(CASE WHEN (EXTRACT(DAY FROM SYSDATE) = 1 AND EXTRACT(hour FROM CAST(SYSDATE AS TIMESTAMP))<8) THEN trunc(SYSDATE)-1 ELSE trunc(SYSDATE) END)),'dd/mm/yyyy') dia_atual
       ,'MÊS' mes
       ,(SELECT sem.semana
              FROM semanas sem
             WHERE data = TRUNC(CASE WHEN (EXTRACT(DAY FROM SYSDATE) = 1 AND EXTRACT(hour FROM CAST(SYSDATE AS TIMESTAMP))<8) THEN trunc(SYSDATE)-1 ELSE trunc(SYSDATE) END)) semana_atual
   FROM dual</$value>
