<$key>query</$key>
<$value>SELECT lo.data data_log
       ,lo.usuario 
       ,lo.acao 
       ,lc.nome_coluna 
       ,lc.old_value 
       ,lc.new_value 
   FROM log_coluna lc 
       ,log_operacao lo 
  WHERE lc.log_operacao_seq = lo.log_operacao_seq 
    AND lc.log_operacao_seq IN (SELECT e.log_operacao_seq 
                                  FROM log_operacao e 
                                      ,log_coluna l 
                                 WHERE NVL(e.tabela, NULL) = 'PED_INF_ADICIONAIS' 
                                   AND e.log_operacao_seq = l.log_operacao_seq 
                                   AND l.nome_coluna = 'PED_NMRO' 
                                   AND l.new_value = TO_CHAR(12837448)) 
    AND lc.nome_coluna <> 'PED_NMRO' 
  ORDER BY lo.data DESC 
          ,lo.log_operacao_seq </$value>
</properties_item>
