<$key>query</$key>
<$value>select distinct cli_rzao, cli_nome, c.est_unifed, ecl_cdad, b.esc_seqn, a.cli_cdgo, r.rep_cdgo, r.rep_rzao, rv.codigo_regional, rv.Nome_Regional   ,b.esc_email, b.esc_cgc from cli a, esc b, ecl c, cliente_representante cr, rep r, regionais_vendas rv where b.esc_seqn=c.esc_seqn AND rv.codigo_regional=r.codigo_regional AND a.cli_cdgo=b.cli_cdgo AND b.cli_cdgo=c.cli_cdgo AND b.cli_cdgo=cr.codigo_cliente AND b.esc_seqn=cr.codigo_sequencial AND(c.ned_cdgo='UNI' or c.ned_cdgo='ENT')AND cr.codigo_representante=r.rep_cdgo AND r.rep_cdgo=38 AND a.cli_rzao like 'Z%'</$value>
