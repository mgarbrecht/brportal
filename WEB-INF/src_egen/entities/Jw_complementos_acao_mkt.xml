<$value>SELECT a.seq_acao_mkt acao
      , t.tipo_complemento
      , c.complemento
   FROM acao_market_dados_adicionais a
       ,acao_marketing_complemento c
       ,acao_marketing_tipo_compl t
  WHERE t.situacao = 'A'
    AND c.situacao = 'A'
    AND t.id = c.id_tipo
    AND a.id_complemento = c.id</$value>
