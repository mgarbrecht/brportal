<$key>query</$key>
<$value>SELECT marca
       ,linha
       ,referencia
       ,cabedal
       ,cor
       ,replace(imagem,'jpg','gif') imagem
       ,cpt.venda_exportacao
       ,cpt.categoria_produto
       ,cpt.descricao_categoria_produto
       ,cpt.tipo
       ,cpt.descricao_tipo
       ,cpt.descricao_produto
	   ,cpt.descricao_cabedal
	   ,cpt.descricao_cor
	   ,sequencial_angulo
   FROM cat_produtos_table cpt
  WHERE codigo_idioma = 3
  ORDER BY marca
          ,linha
          ,referencia
          ,cabedal
          ,cor
