<$key>query</$key>
<$value>SELECT EXTRACT(YEAR FROM ped.ped_daem) ano
       ,SUM(fct_valor_pedido(ite.ped_nmro, ite.ite_seqn)) vlr
       ,SUM(ite.ite_qtde) qtd
   FROM ite
       ,ped
  WHERE ite.ped_nmro = ped.ped_nmro
    AND ped.ped_stat <> 'C'
    AND ped.ped_substituicao <> 'S'
    AND ped.ped_amostra_brinde IS NULL
    AND ((EXTRACT(YEAR FROM ped_daem) = 2011 AND EXTRACT(MONTH FROM ped_daem) BETWEEN 1 AND 12) OR (EXTRACT(YEAR FROM ped_daem) = 2010 AND EXTRACT(MONTH FROM ped_daem) BETWEEN 1 AND 12))
    AND ped.cli_cdgo = 5403
  GROUP BY EXTRACT(YEAR FROM ped.ped_daem)
 </$value>
