<$key>query</$key>
<$value>SELECT /*+ all_rows */
         DISTINCT ecl.cli_cdgo
                 ,ecl.esc_seqn
                 ,ecl.ecl_cdad
                 ,esc.esc_cgc cnpj
                 ,ecl.ecl_nome || ',' || ecl.ecl_nmro || ', Bairro: ' || ecl.ecl_bair || ' , CEP: ' || ecl.ecl_cep endereco
    FROM ecl
        ,esc
   WHERE ecl.ned_cdgo IN ('COB', 'UNI')
     AND esc.cli_cdgo(+) = ecl.cli_cdgo
     AND esc.esc_seqn(+) = ecl.esc_seqn
     AND esc.esc_situacao(+) = 'A'
   ORDER BY ecl.ecl_cdad</$value>
