<properties_item>
<$key>query</$key>
<$value>SELECT DISTINCT c.cli_cdgo
       ,e.esc_seqn
       ,c.cli_rzao
       ,fct_formata_cnpj(e.esc_cgc) cnpj
       ,l.ecl_cdad || ' - ' || l.est_unifed cidade
   FROM ped p
       ,rep r
       ,ite i
       ,cli c
       ,esc e
       ,ecl l
       ,regionais_vendas rv
       ,grupo_economico ge
  WHERE p.ped_nmro = i.ped_nmro
    AND p.cli_cdgo = c.cli_cdgo
    AND e.esc_seqn = p.esc_seqn
    AND e.cli_cdgo = p.cli_cdgo
    AND r.rep_cdgo = p.rep_cdgo
    AND l.cli_cdgo = e.cli_cdgo
    AND l.esc_seqn = e.esc_seqn
    AND rv.codigo_regional = r.codigo_regional
    AND ge.cli_cdgo = c.cli_cdgo
    AND l.ned_cdgo IN ('COB', 'UNI')
    AND p.cli_cdgo = 5403
    AND p.ped_daem > '01-jan-2011'
    AND p.ped_substituicao <> 'S'
    AND p.ped_stat <> 'C'
    AND p.ped_amostra_brinde IS NULL</$value>
