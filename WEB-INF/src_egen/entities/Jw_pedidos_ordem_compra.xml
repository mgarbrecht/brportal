<$key>query</$key>
<$value>SELECT ped.ped_nmro numero
        ,ped.cli_cdgo cliente
		,ped.esc_seqn sequencial
        ,pck_cliente.get_grupo_economico(ped.cli_cdgo) grupo
        ,ped.ped_ped_cliente oc
		,pck_cliente.get_nome(ped.cli_cdgo) cli_rzao
		,pck_grupo_economico.get_nome(pck_cliente.get_grupo_economico(ped.cli_cdgo)) nome_grupo
		,ped.rep_cdgo
        ,pck_representante.get_regional(ped.rep_cdgo) regional
    FROM ped
   WHERE ped.ped_stat NOT IN ('C', 'F')</$value>
